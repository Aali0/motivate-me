<header class="bg-primary text-center">
<button type="submit" onclick="parent.location='index.html'" class="btn btn-default", style="float:right">Return to Home Page</button>
<h1>Quote Favorites</h1>


<div class="cards bg-secondary text-white ">

<ul>
  {{#each quotefavorites}}
    <li>
      <p>
        {{this.id}}. {{this.quote}}

        <button data-quoteid="{{this.id}}" class="delquote btn btn-primary">Delete</button>
      </p>
    </li>
  {{/each}}
</ul>
</div>
<div class="container">
<h2>Add a Favorite Quote</h2>
<form id="addquotefavorite" class="button-size">
	<textarea type="text" name="quotefavorite"></textarea>
  <br />
	<button class="btn btn-primary" type="submit">Save Quote!</button>
</form>
</div>
<body>
	{{{ body }}}
    <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="/favQuotes.js"></script>
	</body>
</html>
